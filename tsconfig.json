{
  "compilerOptions": {
      "target": "ES2021",
      "module": "commonjs",
      "lib": [
          "es6",
          "ES2021",
          "ES2021.String"
      ],
      "outDir": "./gen/srv/srv",
      "rootDir": "./",
      "baseUrl": "./",
      "moduleResolution": "node",
      "skipLibCheck": true,
      "preserveConstEnums": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "inlineSourceMap": true,
      "allowJs": true,
      "strictNullChecks": false,
      "strictPropertyInitialization": false,
      "esModuleInterop": true,
      "paths": {
          "#cds-models/*": [
              "./@cds-models/*/index.ts"
          ]
      }
  },
  "include": [
      "./srv/**/*"
  ]
}